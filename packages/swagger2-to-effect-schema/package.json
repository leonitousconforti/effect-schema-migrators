{
    "name": "swagger2-to-effect-schema",
    "version": "0.0.2",
    "description": "Converts a swagger2 document to an effect schema",
    "homepage": "https://github.com/leonitousconforti/effect-schema-migrators",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/effect-schema-migrators.git",
        "directory": "packages/swagger2-to-effect-schema"
    },
    "license": "MIT",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "bin": {
        "swagger2-to-effect-schema": "dist/bin/migrate.js"
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "_phase:build": "heft run --only build -- --clean",
        "_phase:test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft run --only test -- --clean",
        "build": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" heft test --clean"
    },
    "dependencies": {
        "@effect/cli": "~0.29.0",
        "@effect/platform": "~0.42.4",
        "@effect/platform-node": "~0.41.5",
        "@effect/printer": "~0.30.7",
        "@effect/printer-ansi": "~0.31.7",
        "@effect/typeclass": "~0.22.7",
        "effect-schema-codegen": "workspace:*",
        "json-schema-to-effect-schema": "workspace:*"
    },
    "devDependencies": {
        "@effect/schema": "0.60.4",
        "@rushstack/eslint-config": "3.6.0",
        "@rushstack/heft": "0.64.0",
        "@rushstack/heft-jest-plugin": "0.11.0",
        "@rushstack/heft-lint-plugin": "0.3.0",
        "@rushstack/heft-typescript-plugin": "0.3.0",
        "@types/node": "20.11.5",
        "effect": "2.1.1",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-unicorn": "50.0.1",
        "fast-check": "3.15.0",
        "prettier": "3.2.4",
        "tsconfig-rig": "workspace:*",
        "typescript": "5.3.3"
    },
    "peerDependencies": {
        "@effect/schema": "0.59.1",
        "effect": "2.0.3",
        "fast-check": "3.15.0"
    },
    "engines": {
        "node": ">=16"
    }
